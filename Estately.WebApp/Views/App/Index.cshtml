<div id="section2" class="section">
    <div class="row justify-content-center">
        <div class="col-lg-7 mx-auto text-center">
            <h2 class="mb-4">
                Ensure your representative is an Official Agent by checking their
                phone number below.
            </h2>
            <!-- Button to Open the Modal -->
            <button type="button"
                    class="btn button text-white py-3 px-4"
                    data-bs-toggle="modal"
                    data-bs-target="#exampleModal">
                Verify your Sales Agent
            </button>
            <!-- Modal -->
            <div class="modal fade"
                 id="exampleModal"
                 tabindex="-1"
                 aria-labelledby="exampleModalLabel"
                 aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered">
                    <div class="modal-content">
                        <div class="modal-body">
                            <div class="verify-icon mt-4 mb-3">
                                <i class="fa-solid fa-user-check"></i>
                                <h5 class="mt-3">Verify Your Sales Agent</h5>
                            </div>
                            <div class="verify-form mb-2">
                                <label for="phoneNumber" class="form-label secondary-Text">Enter Agent's Phone Number:</label>
                                <input type="text"
                                       class="form-control"
                                       id="phoneNumber"
                                       placeholder="+20 123 456 7890" />
                            </div>
                        </div>
                        <div class="modal-footer justify-content-center mb-3">
                            <button type="button"
                                    class="btn btn-secondary mt-3"
                                    data-bs-dismiss="modal">
                                Cancel
                            </button>
                            <button type="button" class="btn button mt-3">Verify</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.col-lg-7 -->
    </div>
    <!-- /.row -->
</div>
