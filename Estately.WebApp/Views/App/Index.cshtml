
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="hero">
    <div class="hero-slide">
        <div class="img overlay"
             style="background-image: url('images/hero_bg_3.jpg')"></div>
        <div class="img overlay"
             style="background-image: url('images/hero_bg_2.jpg')"></div>
        <div class="img overlay"
             style="background-image: url('images/hero_bg_1.jpg')"></div>
    </div>

    <div class="container mt-5">
        <div class="row justify-content-center align-items-center">
            <div class="col-lg-12 text-center">
                <h1 class="heading hero-heading" data-aos="fade-up">
                    Discover your place in Egypt's real estate future.
                </h1>
                <!-- Search Form -->
                <div class="row justify-content-center">
                    <div class="col-12 col-lg-10">
                        <div class="search-card">
                            <!-- Desktop Search Interface -->
                            <div class="desktop-search d-none d-lg-block">
                                <!-- Tabs -->
                                <div class="search-tabs mb-4">
                                    <button class="search-tab active" data-tab="compounds">
                                        Compounds
                                    </button>
                                    <button class="search-tab" data-tab="properties">
                                        Properties
                                    </button>
                                </div>

                                <!-- Main Search Input -->
                                <div class="main-search-container mb-4">
                                    <div class="search-input-wrapper">
                                        <i class="fa-solid fa-magnifying-glass search-icon"></i>
                                        <input type="text"
                                               class="main-search-input"
                                               placeholder="Area, Compound, Real Estate Developer" />
                                    </div>
                                </div>

                                <!-- Filter Dropdowns -->
                                <div class="filter-dropdowns d-flex gap-3 mb-4">
                                    <div class="filter-dropdown property-types">
                                        <select class="form-select" aria-label="Property Types">
                                            <option hidden>Property Types</option>
                                            <option value="Apartment">Apartment</option>
                                            <option value="Villa">Villa</option>
                                            <option value="Twinhouse">Twinhouse</option>
                                            <option value="Townhouse">Townhouse</option>
                                            <option value="Duplex">Duplex</option>
                                            <option value="Penthouse">Penthouse</option>
                                            <option value="Chalet">Chalet</option>
                                            <option value="Studio">Studio</option>
                                        </select>
                                    </div>
                                    <div class="filter-dropdown beds-baths">
                                        <select class="form-select" aria-label="Beds and Baths">
                                            <option hidden>Beds and Baths</option>
                                            <option value="1-1">1 Bed, 1 Bath</option>
                                            <option value="2-2">2 Bed, 2 Bath</option>
                                            <option value="3-3">3 Bed, 3 Bath</option>
                                            <option value="4-4">4 Bed, 4 Bath</option>
                                            <option value="5+-5+">5+ Bed, 5+ Bath</option>
                                        </select>
                                    </div>
                                    <div class="filter-dropdown">
                                        <select class="form-select" aria-label="Price Range">
                                            <option hidden>Price Range</option>
                                            <option value="0-500000">$0 - $500,000</option>
                                            <option value="500000-1000000">
                                                $500,000 - $1,000,000
                                            </option>
                                            <option value="1000000-2000000">
                                                $1,000,000 - $2,000,000
                                            </option>
                                            <option value="2000000-5000000">
                                                $2,000,000 - $5,000,000
                                            </option>
                                            <option value="5000000+">$5,000,000+</option>
                                        </select>
                                    </div>
                                    <button class="search-button btn button" type="button">
                                        Search
                                    </button>
                                </div>
                            </div>

                            <!-- Mobile Search Interface -->
                            <div class="mobile-search d-lg-none">
                                <div class="mobile-search-container">
                                    <div class="mobile-search-input-wrapper">
                                        <i class="fa-solid fa-magnifying-glass mobile-search-icon"></i>
                                        <input type="text"
                                               class="mobile-search-input"
                                               placeholder="Area, Compound, Developer" />
                                        <button class="mobile-filter-button"
                                                type="button"
                                                data-bs-toggle="modal"
                                                data-bs-target="#filterModal">
                                            <i class="fa-solid fa-sliders-h"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Filter Modal -->
<div class="modal fade search-filter"
     id="filterModal"
     tabindex="-1"
     aria-labelledby="filterModalLabel"
     aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <div class="d-flex align-items-center">
                    <i class="fa-solid fa-sliders-h me-2"></i>
                    <h5 class="modal-title" id="filterModalLabel">Filters</h5>
                </div>
                <button type="button"
                        class="btn-close"
                        data-bs-dismiss="modal"
                        aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <!-- Areas Section -->
                <div class="filter-section">
                    <div class="filter-header">
                        <h6 class="filter-title">Areas</h6>
                        <button class="reset-btn">Reset</button>
                    </div>
                    <div class="filter-content">
                        <div class="add-areas-input">
                            <i class="fa-solid fa-plus text-success me-2"></i>
                            <input type="text"
                                   class="form-control"
                                   placeholder="Add Areas" />
                        </div>
                    </div>
                </div>

                <!-- Developers Section -->
                <div class="filter-section">
                    <div class="filter-header">
                        <h6 class="filter-title">Developers</h6>
                        <button class="reset-btn">Reset</button>
                    </div>
                    <div class="filter-content">
                        <div class="add-developers-input">
                            <i class="fa-solid fa-plus text-success me-2"></i>
                            <input type="text"
                                   class="form-control"
                                   placeholder="Add Developers" />
                        </div>
                    </div>
                </div>

                <!-- Property Types Section -->
                <div class="filter-section">
                    <div class="filter-header">
                        <h6 class="filter-title">Property Types</h6>
                        <button class="reset-btn">Reset</button>
                    </div>
                    <div class="filter-content">
                        <div class="property-types-grid">
                            <button class="property-type-btn">Apartment</button>
                            <button class="property-type-btn">Villa</button>
                            <button class="property-type-btn">Twinhouse</button>
                            <button class="property-type-btn">Townhouse</button>
                            <button class="property-type-btn">Duplex</button>
                            <button class="property-type-btn">Penthouse</button>
                            <button class="property-type-btn">Chalet</button>
                            <button class="property-type-btn">Studio</button>
                            <button class="property-type-btn">Cabin</button>
                            <button class="property-type-btn">Office</button>
                            <button class="property-type-btn">Clinic</button>
                            <button class="property-type-btn">Retail</button>
                            <button class="property-type-btn">Serviced Studio</button>
                            <button class="property-type-btn">Branded Apartment</button>
                            <button class="property-type-btn">Branded Villa</button>
                            <button class="property-type-btn">Luxury Apartment</button>
                            <button class="property-type-btn">
                                Ultra Luxury Apartment
                            </button>
                            <button class="property-type-btn">Ultra Luxury Villa</button>
                            <button class="property-type-btn">One-story Villa</button>
                            <button class="property-type-btn">Pharmacy</button>
                        </div>
                    </div>
                </div>
                <!-- Price Section -->
                <div class="filter-section">
                    <div class="filter-header">
                        <h6 class="filter-title">Price</h6>
                        <button class="reset-btn">Reset</button>
                    </div>
                    <div class="filter-content">
                        <div class="range-slider-container">
                            <input type="range"
                                   class="range-slider"
                                   min="0"
                                   max="100000000"
                                   value="5000000"
                                   id="priceMin" />
                            <input type="range"
                                   class="range-slider"
                                   min="0"
                                   max="100000000"
                                   value="25000000"
                                   id="priceMax" />
                        </div>
                        <div class="range-inputs d-flex gap-3 mt-3">
                            <div class="range-input">
                                <input type="number"
                                       class="form-control"
                                       value="5000000"
                                       id="priceMinInput" />
                                <span class="unit">L.E</span>
                            </div>
                            <div class="range-input">
                                <input type="number"
                                       class="form-control"
                                       value="25000000"
                                       id="priceMaxInput" />
                                <span class="unit">L.E</span>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Unit Area Section -->
                <div class="filter-section">
                    <div class="filter-header">
                        <h6 class="filter-title">Unit Area</h6>
                        <button class="reset-btn">Reset</button>
                    </div>
                    <div class="filter-content">
                        <div class="range-slider-container">
                            <input type="range"
                                   class="range-slider"
                                   min="0"
                                   max="1000"
                                   value="50"
                                   id="areaMin" />
                            <input type="range"
                                   class="range-slider"
                                   min="0"
                                   max="1000"
                                   value="400"
                                   id="areaMax" />
                        </div>
                        <div class="range-inputs d-flex gap-3 mt-3">
                            <div class="range-input">
                                <input type="number"
                                       class="form-control"
                                       value="50"
                                       id="areaMinInput" />
                                <span class="unit">m2</span>
                            </div>
                            <div class="range-input">
                                <input type="number"
                                       class="form-control"
                                       value="400"
                                       id="areaMaxInput" />
                                <span class="unit">m2</span>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Bedrooms Section -->
                <div class="filter-section">
                    <div class="filter-header">
                        <h6 class="filter-title">Bedrooms</h6>
                        <button class="reset-btn">Reset</button>
                    </div>
                    <div class="filter-content">
                        <div class="bedroom-bathroom-options">
                            <button class="option-btn">1</button>
                            <button class="option-btn">2</button>
                            <button class="option-btn">3</button>
                            <button class="option-btn">4</button>
                            <button class="option-btn">5+</button>
                        </div>
                    </div>
                </div>

                <!-- Bathrooms Section -->
                <div class="filter-section">
                    <div class="filter-header">
                        <h6 class="filter-title">Bathrooms</h6>
                        <button class="reset-btn">Reset</button>
                    </div>
                    <div class="filter-content">
                        <div class="bedroom-bathroom-options">
                            <button class="option-btn">1</button>
                            <button class="option-btn">2</button>
                            <button class="option-btn">3</button>
                            <button class="option-btn">4</button>
                            <button class="option-btn">5+</button>
                        </div>
                    </div>
                </div>

                <!-- Amenities Section -->
                <div class="filter-section">
                    <div class="filter-header">
                        <h6 class="filter-title">Amenities</h6>
                        <button class="reset-btn">Reset</button>
                    </div>
                    <div class="filter-content">
                        <div class="amenities-grid">
                            <button class="amenity-btn">Garden</button>
                            <button class="amenity-btn">Has roof</button>
                            <button class="amenity-btn">bicycles lanes</button>
                            <button class="amenity-btn">Disability support</button>
                            <button class="amenity-btn">Jogging trail</button>
                            <button class="amenity-btn">Outdoor pools</button>
                            <button class="amenity-btn">Mosque</button>
                            <button class="amenity-btn">Sports Clubs</button>
                            <button class="amenity-btn">Business Hub</button>
                            <button class="amenity-btn">Commercial strip</button>
                            <button class="amenity-btn">Medical center</button>
                            <button class="amenity-btn">Schools</button>
                            <button class="amenity-btn">Underground parking</button>
                            <button class="amenity-btn">Clubhouse</button>
                            <button class="amenity-btn">Terrace</button>
                            <button class="amenity-btn">Sea View</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button"
                        class="btn btn-outline-secondary"
                        data-bs-dismiss="modal">
                    Cancel
                </button>
                <button type="button" class="btn btn-primary search-btn">
                    Search
                </button>
            </div>
        </div>
    </div>
</div>
